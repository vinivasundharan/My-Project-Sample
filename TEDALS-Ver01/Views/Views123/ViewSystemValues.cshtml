@model TEDALS_Ver01.ViewModels.ModelToBePassed

@{
    ViewBag.Title = "View Values";
}

<ul class="breadcrumb">
    You are Here :
    <li>@Html.ActionLink("Home", "Index", "Home")</li>
    <li>@Html.ActionLink("Views", "Index", "Views123")</li>
    <li>System View Values</li>
</ul>

<h2>System View Values</h2>
<div class="col-md-2">
    <ul class="nav navbar-nav">
        <li>@Html.ActionLink("View all views", "Index", "Views123")</li>
        <li>@Html.ActionLink("Add View", "Create", "Views123")</li>
        <li>@Html.ActionLink("View all systems", "Index", "Lsystems")</li>
    </ul>
</div>
<div class="col-md-10">
    <h4>
        System Name : @ViewBag.LsystemName
        <br />
        View Name : @ViewBag.ViewsName
    </h4>

    <table class="table">
        <tr>
            @foreach (var item in Model.TcSet)
            {
                <th>@item.SetName</th>

            }
        </tr>
        @foreach (var item in Model.ViewsVM)
        {
            <tr>
                @for (int i = 0; i < item.row.Count(); i++)
                {
                    <td>@item.row[i]</td>
                }
            </tr>
        }
    </table>
    <p>
        @Html.ActionLink("Back to List","Index","Views123")
    </p>

</div>